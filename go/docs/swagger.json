                    }
                },
                "teams": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/github_com_gladom_beerpong_pkg_models.Team"
                    }
                }
            }
        },
        "github_com_gladom_beerpong_pkg_models.Match": {
            "type": "object",
            "properties": {
                "away_team": {
                    "type": "string"
                },
                "created_at": {
                    "type": "string"
                },
                "game_id": {
                    "type": "integer"
                },
                "group_number": {
                    "type": "string"
                },
                "home_team": {
                    "type": "string"
                },
                "points_away": {
                    "type": "integer"
                },
                "points_home": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                },
                "updated_at": {
                    "type": "string"
                }
            }
        },
        "github_com_gladom_beerpong_pkg_models.MatchUpdateRequest": {
            "type": "object",
            "properties": {
                "matches": {
                    "type": "array",
