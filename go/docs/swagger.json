        "github_com_gladom_beerpong_pkg_models.Game": {
            "type": "object",
            "properties": {
                "amount_of_teams": {
                    "type": "integer"
                },
                "created_at": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "is_finished": {
                    "type": "boolean"
                },
                "mode": {
                    "type": "integer"
                },
                "updated_at": {
                    "type": "string"
                }
            }
        },
        "github_com_gladom_beerpong_pkg_models.GameResponse": {
            "type": "object",
